<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.solvd.dijkstra.batis.ICity">
	<resultMap id="cityMapper"
               type="com.solvd.dijkstra.batis.City" autoMapping="false">
		
		<result property="name" column="name" />

	</resultMap>

	<select id="getAll" resultMap="cityMapper">
		select * from cities
	</select>





</mapper>