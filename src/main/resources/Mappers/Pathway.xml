<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.solvd.dijkstra.batis.IPathway">

	<resultMap id="pathwayMapper"
			   type="com.solvd.dijkstra.batis.Pathway" autoMapping="false">

		<result property="start" column="start" />
		<result property="end" column="end" />
		<result property="distance" column="distance" />
	</resultMap>

	<select id="getAll" resultMap="pathwayMapper">
		select * from pathway
	</select>


</mapper>